    <h2>Resultado</h2>
    <div>
      <h3>Ejemplo</h3>
      <!-- ------ start text ------ -->
      <div id='very'>
        <!-- :::::::::::::::::::::::::::::::::::::::::::::: -->
<p id="demo4"></p>

<script>
var carName14 = "Volvo XC60";
var carName24 = 'Volvo XC60';
var answer14 = "It's alright";
var answer24 = "He is called 'Johnny'";
var answer34 = 'He is called "Johnny"';

document.getElementById("demo4")
  .innerHTML =
      carName14 + "<br>" + carName24 + "<br>" + 
      answer14  + "<br>" + answer24  + "<br>" +   answer34;
</script>        


        <!-- :::::::::::::::::::::::::::::::::::::::::::::: -->
        <!-- ------ end text -------- -->
      </div>
    </div>
  </article>
  <article>
    <h2>Codigo</h2>
    <div>
      <h3>HTML</h3>
      <!-- ------ start text ------ -->
      <pre id='gray'>&lt;body>
      <!-- :::::::::::::::::::::::::::::::::::::::::::::: -->
&lt;p>

&lt;/p>      
&lt;p id="demo">&lt;/p>      
      <!-- :::::::::::::::::::::::::::::::::::::::::::::: -->
      &lt;/body></pre>
      <!-- ------ end html -------- -->
      <hr>
      <!-- ------ begin -------- -->
      <h3>JS</h3>
      <!-- ------ start js -------- -->
      <pre id='gray'>&lt;script>
      <!-- :::::::::::::::::::::::::::::::::::::::::::::: -->
var carName1 = "Volvo XC60";
var carName2 = 'Volvo XC60';
var answer1 = "It's alright";
var answer2 = "He is called 'Johnny'";
var answer3 = 'He is called "Johnny"';

document.getElementById("demo")
  .innerHTML =
      carName1 + "&lt;br>" + carName2 + "&lt;br>" + 
      answer1  + "&lt;br>" + answer2  + "&lt;br>" +   answer3;
      <!-- :::::::::::::::::::::::::::::::::::::::::::::: -->
      &lt;/script></pre>
    </div>
  </article>
</div>
