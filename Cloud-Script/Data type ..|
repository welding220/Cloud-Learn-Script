    .innerHTML =
        typeof undefined     + "<br>" +  typeof null + "<br>" +
        (null === undefined) + "<br>" +   (null == undefined);
</script>

        <!-- :::::::::::::::::::::::::::::::::::::::::::::: -->
        <!-- ------ end text -------- -->
      </div>
    </div>
