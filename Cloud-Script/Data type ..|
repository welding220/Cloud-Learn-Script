        "The value is: " + car + "&lt;br>" +
        "The type is: " + typeof car;
      <!-- :::::::::::::::::::::::::::::::::::::::::::::: -->
      &lt;/script></pre>
    </div>
  </article>
</div>
<hr>
<hr>
<!-- :::::::::::::::::::::::::::::::::::::::::::::::::::::::: -->
<!-- -----------------------  end  ------------------------- -->
<!-- :::::::::::::::::::::::::::::::::::::::::::::::::::::::: -->




