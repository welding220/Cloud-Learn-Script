<p id="demo7"></p>

<script>
var person7 = {
    firstName7 : "John",
    lastName7  : "Doe",
    age7       : 50,
    eyeColor7  : "blue"
};

document.getElementById("demo7")
    .innerHTML =
        person7.firstName7 + " is " + person7.age7 + " years old.";
</script>

        <!-- :::::::::::::::::::::::::::::::::::::::::::::: -->
        <!-- ------ end text -------- -->
      </div>
