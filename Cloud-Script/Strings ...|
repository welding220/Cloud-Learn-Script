<p id="demo9"></p>

<script>
var x9 = "John";              // x is a string
var y9 = new String("John");  // y is an object
document.getElementById("demo9").innerHTML = (x9==y9);
</script>



        <!-- :::::::::::::::::::::::::::::::::::::::::::::: -->
        <!-- ------ end text -------- -->
      </div>
